package BreezeTask;

import java.util.Date;

import BreezeTask.Content;

public class Notes implements Content{
    private String text;
    private Date date;
    private int number;

    public Notes(String text, Date date, int number) {
        this.text = text;
        this.date = date;
        this.number = number;
    }

    public String getContent() {
        return text;
    }

    public Date getDate() {
        return date;
    }

    public Boolean isEmpty() {
        return text.equals("");
    }
}
